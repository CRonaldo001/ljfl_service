<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.modules.AppSchool.dao.AppSchoolDao">

    <resultMap type="io.renren.modules.AppSchool.entity.AppSchoolEntity" id="appSchoolMap">
        <result property="id" column="id"/>
        <result property="tenantCode" column="tenant_code"/>
        <result property="creator" column="creator"/>
        <result property="createDate" column="create_date"/>
        <result property="updater" column="updater"/>
        <result property="updateDate" column="update_date"/>
        <result property="schoolName" column="school_name"/>
        <result property="schoolCode" column="school_code"/>
    </resultMap>
    <select id="getListSchool" resultType="io.renren.modules.AppSchool.dto.AppSchoolDTO">

             select  * from  app_school


    </select>

</mapper>